[run]
source = .
omit = 
    */tests/*
    */test_*.py
    */migrations/*
    */venv/*
    */__pycache__/*
    */setup.py
    */conftest.py
    */wsgi.py
    */Dockerfile*
    */docker-compose*.yml

[report]
# Minimum coverage thresholds
precision = 2
show_missing = True
skip_covered = False

exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov

